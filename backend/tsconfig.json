{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    // "strict": true,
    "outDir": "./dist"
  },
  "exclude": [
    "services/**/*",
    "node_modules",
    "dist",
    "_test_/**/*",
    "_test_/*",

  ]
}
